{
  "MoAI": {
    "Server": "${MOAI_SERVER_URL}",
    "WebUI": "${MOAI_WEBUI_URL}",
    "AES": "${MOAI_AES_KEY}",
    "DBType": "postgres",
    "Database": "Database=${POSTGRES_DB};Host=postgres;Password=${POSTGRES_PASSWORD};Port=5432;Username=${POSTGRES_USER};Search Path=public",
    "Redis": "redis:6379",
    "Wiki": {
      "DBType": "postgres",
      "Database": "Database=${POSTGRES_DB};Host=postgres;Password=${POSTGRES_PASSWORD};Port=5432;Username=${POSTGRES_USER};Search Path=public"
    },
    "RabbitMQ": "amqp://${RABBITMQ_USER}:${RABBITMQ_PASSWORD}@rabbitmq:5672",
    "Storage": {
      "LocalPath": "/app/files"
    },
    "OTLP": {
      "Trace": "http://127.0.0.1:4012/v1/traces",
      "Metrics": "http://127.0.0.1:4012/v1/metrics",
      "Protocol": 0
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore.HttpLogging": "Information",
        "ProtoBuf.Grpc.Server.ServicesExtensions.CodeFirstServiceMethodProvider": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System.Net.Http.HttpClient.TenantManagerClient.LogicalHandler": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command.CommandExecuted": "Warning",
        "System": "Information",
        "Microsoft": "Warning",
        "Grpc": "Information",
        "MySqlConnector": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{SourceContext} {Scope} {Timestamp:HH:mm} [{Level}]{NewLine}{Message:lj} {Exception}{NewLine}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}